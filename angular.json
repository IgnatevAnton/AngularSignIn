{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"AngularSignIn": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"standalone": false,
					"style": "scss"
				},
				"@schematics/angular:directive": {
					"standalone": false
				},
				"@schematics/angular:pipe": {
					"standalone": false
				}
			},
			"root": "",
			"sourceRoot": "src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "@ngx-env/builder:application",
					"options": {
						"outputPath": "dist/angular-sign-in",
						"index": "src/index.html",
						"browser": "src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							}
						],
						"styles": [
							"src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "4kB",
									"maximumError": "8kB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@ngx-env/builder:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "AngularSignIn:build:production"
						},
						"development": {
							"buildTarget": "AngularSignIn:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 63953,
						"ssl": true,
						"sslKey": "localhost.key",
						"sslCert": "localhost.crt"
					}
				},
				"extract-i18n": {
					"builder": "@ngx-env/builder:extract-i18n",
					"options": {}
				},
				"test": {
					"builder": "@ngx-env/builder:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							}
						],
						"styles": [
							"src/styles.scss"
						],
						"scripts": [],
						"karmaConfig": "karma.conf.js",
						"builderMode": "browser"
					}
				}
			}
    },
    "domain": {
      "projectType": "library",
      "root": "projects/domain",
      "sourceRoot": "projects/domain/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/domain/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/domain/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/domain/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/domain/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "application": {
      "projectType": "library",
      "root": "projects/application",
      "sourceRoot": "projects/application/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/application/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/application/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/application/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/application/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "infrastructure": {
      "projectType": "library",
      "root": "projects/infrastructure",
      "sourceRoot": "projects/infrastructure/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/infrastructure/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/infrastructure/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/infrastructure/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/infrastructure/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
	},
	"cli": {
		"analytics": "55a4a770-bd8c-42b6-87f1-f882bb039827"
	}
}
